{
  "name": "react-props-generator",
  "version": "0.0.2",
  "description": "Generate default props and fake props data base on React components's PropTypes",
  "main": "build/index.js",
  "scripts": {
    "start": "react-app-rewired start",
    "build": "rimraf build && babel src/lib -d build",
    "prepublishOnly": "npm run build && pkg-ok",
    "precommit": "npm test",
    "test": "standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hicoldcat/react-props-generator.git"
  },
  "keywords": [
    "react",
    "props",
    "proptypes",
    "generate",
    "generator",
    "defaultprops",
    "fake data",
    "prop"
  ],
  "author": "hicoldcat",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hicoldcat/react-props-generator/issues"
  },
  "homepage": "https://github.com/hicoldcat/react-props-generator#readme",
  "dependencies": {
    "babel-core": "^7.0.0-bridge",
    "lodash.clonedeep": "^4.5.0",
    "mockjs": "^1.0.1-beta3",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-app-rewired": "^1.6.2",
    "react-dom": "^16.6.3",
    "react-props-generator": "0.0.2",
    "react-scripts": "2.1.1"
  },
  "devDependencies": {
    "@babel/cli": "7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.6",
    "babel-plugin-import": "^1.11.0",
    "customize-cra": "^0.2.7",
    "pkg-ok": "^2.3.1",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.2",
    "standard": "^12.0.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
